{
  "name": "adk-stream-protocol",
  "version": "1.0.2",
  "description": "AI SDK v6 and Google ADK integration example with data stream protocol",
  "scripts": {
    "dev": "next dev -p 3000",
    "build": "next build",
    "start": "next start -p 3000",
    "lint": "biome check --write .",
    "format": "biome format --write .",
    "test:lib": "vitest run lib/",
    "test:app": "vitest run app/",
    "test:components": "vitest run components/",
    "test:e2e": "playwright test --global-timeout=600000",
    "test:e2e:scenarios": "playwright test --project=scenarios --global-timeout=600000",
    "test:e2e:app": "playwright test --project=app-e2e-smoke --project=app-e2e-core --project=app-e2e-advanced",
    "test:e2e:app:smoke": "playwright test --project=app-e2e-smoke",
    "test:e2e:app:core": "playwright test --project=app-e2e-core",
    "test:e2e:app:advanced": "playwright test --project=app-e2e-advanced",
    "test:e2e:ui": "playwright test --ui"
  },
  "keywords": [],
  "author": "hironow",
  "license": "MIT",
  "dependencies": {
    "@ai-sdk/google": "3.0.1",
    "@ai-sdk/react": "3.0.3",
    "@valibot/to-json-schema": "^1.5.0",
    "ai": "6.0.3",
    "effect": "^3.19.11",
    "next": "^16.0.10",
    "react": "^19.2.3",
    "react-dom": "^19.2.3",
    "zod": "^4.1.13"
  },
  "devDependencies": {
    "@axe-core/playwright": "^4.11.0",
    "@biomejs/biome": "^2.3.8",
    "@playwright/test": "^1.57.0",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/node": "^25.0.1",
    "@types/react": "^19.2.7",
    "@types/react-dom": "^19.2.3",
    "dotenv": "^17.2.3",
    "jsdom": "^27.3.0",
    "msw": "^2.12.4",
    "typescript": "^5.9.3",
    "vitest": "^4.0.16"
  }
}
