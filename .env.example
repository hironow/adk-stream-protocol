# Gemini API Key (for frontend - AI SDK v6 uses this)
GOOGLE_GENERATIVE_AI_API_KEY=

# Gemini API Key (for ADK backend - ADK uses this env var name)
GOOGLE_API_KEY=

# ADK Configuration - Use Google AI Studio (not Vertex AI)
# Set to 0 for Google AI Studio, 1 for Vertex AI
GOOGLE_GENAI_USE_VERTEXAI=0

# Server Configuration
BACKEND_PORT=8000
FRONTEND_PORT=3000

# Backend Mode (gemini | adk-sse)
BACKEND_MODE=adk-sse
NEXT_PUBLIC_BACKEND_MODE=gemini

# ADK Backend URL
ADK_BACKEND_URL=http://localhost:8000
NEXT_PUBLIC_ADK_BACKEND_URL=http://localhost:8000

# Debug flags
NEXT_PUBLIC_DEBUG_CHAT_OPTIONS=true

# Debug chunk flags
CHUNK_LOGGER_ENABLED=true
# CHUNK_LOGGER_SESSION_ID="session-1"
NEXT_PUBLIC_CHUNK_LOGGER_ENABLED=true
# NEXT_PUBLIC_CHUNK_LOGGER_SESSION_ID="session-1"

E2E_CHUNK_PLAYER_MODE=false

OPENWEATHERMAP_API_KEY=
